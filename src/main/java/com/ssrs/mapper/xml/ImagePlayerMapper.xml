<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssrs.mapper.ImagePlayerMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.ssrs.model.ImagePlayer">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="width" property="width" />
        <result column="height" property="height" />
        <result column="swiper_type" property="swiperType" />
        <result column="display_count" property="displayCount" />
        <result column="display_desc" property="displayDesc" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, width, height, swiper_type, display_count, display_desc
    </sql>

</mapper>
