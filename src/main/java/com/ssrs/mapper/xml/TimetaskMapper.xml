    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, group_name, start_time, end_time, cron, job_status, plan_status, is_concurrent, job_data, method_name, bean_name, description, create_user_id, create_time, modify_user_id, modify_time
